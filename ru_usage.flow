import net/url_parameter;
import ru_conf;

export {
	ruUsage(conf : RuConf) -> void {
		prih = \line -> if (lookupTreeDef(conf.opts, "help", "0") == "1") println(line);
		pri  = \line -> println(line);
		pri ("Usage: russell [opts] file");
		pri ("    file=<name>   another way to setup a file");
		pri ("    help=1        print a help");
		prih("    verbose=1     non-silent mode");
	}
}
