# Merge a current lib state with the other

read;
update-conf verbose=2;
merge-math 
    file=$afile
    importRoots=$impRoots
    exportRoot=$expRoot
    info-verbose=1
    merge=1
    verify=smart
    ;
update-conf verbose=2;
remove-empty-sources;
update-conf verbose=1;
#verify all;
write-ru all=1;
