export {
	ruCommandExpressionLenStats(math : RuMath) -> void;
	ruCommandReproveAllStats(math : RuMath) -> void;
	ruCommandProofsStats(math : RuMath) -> void;
	ruCommandReproveTimeStats(math : RuMath) -> void;
	ruCommandTheoremDepthStats(math : RuMath) -> void;
}

ruCommandExpressionLenStats(math : RuMath) -> void {
	traverseInOrder(ruMathTheorems(math), \__, th ->
		iter(th.proof.steps, \step ->
			ruAddStats("expr_lens", RuStatsRow(
					ru2s(step.expr, math.rules), 
					pairs2tree([Pair("size", i2d(length(step.expr.nodes))), Pair("count", 1.0)])
				)
			)
		)
	);
	ruPrintln("<< expression lens stats >>");
	ruPrintln("distrib lens: ");
	ruPrintln(ruIndent(ruProcStats("expr_lens", ruDistribLinSumStats("size", "count", 10))));
	ruPrintln("max size:   " + ruProcStats("expr_lens", ruMaxStats("size")));
	ruPrintln("avg size:   " + ruProcStats("expr_lens", ruAvgStats("size")));
	ruPrintln("sigma size: " + ruProcStats("expr_lens", ruSigmaStats("size")));
	ruPrintln("");
}

ruCommandProofsStats(math : RuMath) -> void {
	traverseInOrder(ruMathTheorems(math), \__, th ->
		ruAddStats("proof_stats", RuStatsRow(
				th.name, 
				pairs2tree([
					Pair("size", i2d(length(th.proof.steps))), 
					Pair("vol", i2d(fold(th.proof.steps, 0, \acc, step ->
						acc + length(step.expr.nodes)
					))),
					Pair("count", 1.0)
				])
			)
		)
	);
	ruPrintln("<< proof stats >>");
	ruPrintln("distrib proofs size: ");
	ruPrintln(ruIndent(ruProcStats("proof_stats", ruDistribLogSumStats("size", "count", 2))));
	ruPrintln("");
	ruPrintln("distrib proofs vol: ");
	ruPrintln(ruIndent(ruProcStats("proof_stats", ruDistribLogSumStats("vol", "count", 2))));
	ruPrintln("");
	ruPrintln("distrib proofs (size - vol): ");
	ruPrintln(ruIndent(ruProcStats("proof_stats", ruDistribLogSumStats("size", "vol", 10))));
	ruPrintln("");
	ruPrintln("distrib proofs (vol - size): ");
	ruPrintln(ruIndent(ruProcStats("proof_stats", ruDistribLogSumStats("vol", "size", 10))));
	ruPrintln("");
	ruPrintln("num size:   " + ruProcStats("proof_stats", ruNumStats("size")));
	ruPrintln("max size:   " + ruProcStats("proof_stats", ruMaxStats("size")));
	ruPrintln("avg size:   " + ruProcStats("proof_stats", ruAvgStats("size")));
	ruPrintln("sigma size: " + ruProcStats("proof_stats", ruSigmaStats("size")));
	ruPrintln("");
	ruPrintln("num vol:   " + ruProcStats("proof_stats", ruNumStats("vol")));
	ruPrintln("max vol:   " + ruProcStats("proof_stats", ruMaxStats("vol")));
	ruPrintln("avg vol:   " + ruProcStats("proof_stats", ruAvgStats("vol")));
	ruPrintln("sigma vol: " + ruProcStats("proof_stats", ruSigmaStats("vol")));
	ruPrintln("");
}

