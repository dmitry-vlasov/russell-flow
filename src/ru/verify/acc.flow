import ru/subst;
import ru/verify/step;

export {
	RuVerifyAcc(
		math : RuMath,
		approved  : Set<int>,
		disproved : Set<int>
	);
	ruMergeVerifyAcc(a1 : RuVerifyAcc, a2 : RuVerifyAcc, env : RuEnv) -> RuVerifyAcc;
}

ruMergeVerifyAcc(a1 : RuVerifyAcc, a2 : RuVerifyAcc, env : RuEnv) -> RuVerifyAcc {
	RuVerifyAcc(
		ruMergeMath(a1.math, a2.math, env),
		mergeSets(a1.approved, a2.approved),
		mergeSets(a1.disproved, a2.disproved)
	);
}
