import ru/subst;
import ru/verify/step;
import command;

export {
	RuVerifyAcc(
		math : RuMath,
		approved  : Set<int>,
		disproved : Set<int>
	);
	ruMergeVerifyAcc(a1 : RuVerifyAcc, a2 : RuVerifyAcc, conf : RuConf) -> RuVerifyAcc;
}

ruMergeVerifyAcc(a1 : RuVerifyAcc, a2 : RuVerifyAcc, conf : RuConf) -> RuVerifyAcc {
	RuVerifyAcc(
		ruMergeMath(a1.math, a2.math, conf),
		mergeSets(a1.approved, a2.approved),
		mergeSets(a1.disproved, a2.disproved)
	);
}
