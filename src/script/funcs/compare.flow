import script/func;
import script/util;

export {
	ruScriptCompareFuncs() -> [ScriptFn];
}

ruScriptCompareFuncs() -> [ScriptFn] {
	[
		ScriptFn("==", \args, state, out -> {
			if (ruHasStringArg(args)) {
				ruEvalToStringFn(args, \as -> Some(scBoolVal(as[0] == as[1])), "==", 2);
			} else {
				if (!ruAllArgsAreOfSameType(args)) {
					ScValue(["comparing values of different types: " + ruScValues2s(args)], scErrorType);
				} else {
					first_arg_type = scTypeGen(args[0].type);
					if (first_arg_type == "bool")   ruEvalBoolFn(args,   \as -> Some(scBoolVal(as[0] == as[1])), "==", 2) else 
					if (first_arg_type == "int")    ruEvalIntFn(args,    \as -> Some(scBoolVal(as[0] == as[1])), "==", 2) else 
					if (first_arg_type == "double") ruEvalDoubleFn(args, \as -> Some(scBoolVal(as[0] == as[1])), "==", 2) else 
					if (first_arg_type == "array")  ruEvalArrayFn(args,  \as -> Some(scBoolVal(as[0] == as[1])), "==", 2) else 
					if (first_arg_type == "map")    ruEvalTreeFn(args,   \as -> Some(scBoolVal(as[0] == as[1])), "==", 2) else 
					//if (first_arg_type == "wrap")   ruEvalDataFn(args,   \as -> Some(scBoolVal(as[0] == as[1])), "==", 2) else
					ScValue(["operator == is undefined on arguments: " + ruScValues2s(args)], scErrorType);
					/*switch (args[0]) {
						ScBool(__):    ruEvalBoolFn(args,   \as -> Some(ScBool(as[0] == as[1])), "==", 2);
						ScInt(__):     ruEvalIntFn(args,    \as -> Some(ScBool(as[0] == as[1])), "==", 2);
						ScDouble(__):  ruEvalDoubleFn(args, \as -> Some(ScBool(as[0] == as[1])), "==", 2);
						ScArray(__):   ruEvalArrayFn(args,  \as -> Some(ScBool(as[0] == as[1])), "==", 2);
						ScMap(__):     ruEvalTreeFn(args,   \as -> Some(ScBool(as[0] == as[1])), "==", 2);
						ScData(__,__): ruEvalDataFn(args,   \as -> Some(ScBool(as[0] == as[1])), "==", 2);
						default: ScError(["operator == is undefined on arguments: " + ruScValues2s(args)]);
					}*/
				}
			}
		}),
		ScriptFn("!=", \args, state, out -> {
			if (ruHasStringArg(args)) {
				ruEvalToStringFn(args, \as -> Some(scBoolVal(as[0] != as[1])), "!=", 2);
			} else {
				if (!ruAllArgsAreOfSameType(args)) {
					ScValue(["comparing values of different types: " + ruScValues2s(args)], scErrorType);
				} else {
					first_arg_type = scTypeGen(args[0].type);
					if (first_arg_type == "bool")   ruEvalBoolFn(args,   \as -> Some(scBoolVal(as[0] != as[1])), "!=", 2) else 
					if (first_arg_type == "int")    ruEvalIntFn(args,    \as -> Some(scBoolVal(as[0] != as[1])), "!=", 2) else 
					if (first_arg_type == "double") ruEvalDoubleFn(args, \as -> Some(scBoolVal(as[0] != as[1])), "!=", 2) else 
					if (first_arg_type == "array")  ruEvalArrayFn(args,  \as -> Some(scBoolVal(as[0] != as[1])), "!=", 2) else 
					if (first_arg_type == "map")    ruEvalTreeFn(args,   \as -> Some(scBoolVal(as[0] != as[1])), "!=", 2) else 
					//if (first_arg_type == "wrap")   ruEvalDataFn(args,   \as -> Some(scBoolVal(as[0] != as[1])), "!=", 2) else
					ScValue(["operator != is undefined on arguments: " + ruScValues2s(args)], scErrorType);
					/*switch (args[0]) {
						ScBool(__):    ruEvalBoolFn(args,   \as -> Some(ScBool(as[0] != as[1])), "!=", 2);
						ScInt(__):     ruEvalIntFn(args,    \as -> Some(ScBool(as[0] != as[1])), "!=", 2);
						ScDouble(__):  ruEvalDoubleFn(args, \as -> Some(ScBool(as[0] != as[1])), "!=", 2);
						ScArray(__):   ruEvalArrayFn(args,  \as -> Some(ScBool(as[0] != as[1])), "!=", 2);
						ScMap(__):     ruEvalTreeFn(args,   \as -> Some(ScBool(as[0] != as[1])), "!=", 2);
						ScData(__,__): ruEvalDataFn(args,   \as -> Some(ScBool(as[0] != as[1])), "!=", 2);
						default: ScError(["operator != is undefined on arguments: " + ruScValues2s(args)]);
					}*/
				}
			}
		}),
		ScriptFn("<=", \args, state, out -> {
			if (ruHasStringArg(args)) {
				ruEvalToStringFn(args, \as -> Some(scBoolVal(as[0] <= as[1])), "<=", 2);
			} else {
				first_arg_type = scTypeGen(args[0].type);
				if (first_arg_type == "int")    ruEvalIntFn(args,    \as -> Some(scBoolVal(as[0] <= as[1])), "<=", 2) else 
				if (first_arg_type == "double") ruEvalDoubleFn(args, \as -> Some(scBoolVal(as[0] <= as[1])), "<=", 2) else 
				ScValue(["operator <= is undefined on arguments: " + ruScValues2s(args)], scErrorType);
				/*switch (args[0]) {
					ScInt(__):    ruEvalIntFn(args,    \as -> Some(ScBool(as[0] <= as[1])), "<=", 2);
					ScDouble(__): ruEvalDoubleFn(args, \as -> Some(ScBool(as[0] <= as[1])), "<=", 2);
					default: ScError(["operator <= is undefined on arguments: " + ruScValues2s(args)]);
				}*/
			}
		}),
		ScriptFn(">=", \args, state, out -> {
			if (ruHasStringArg(args)) {
				ruEvalToStringFn(args, \as -> Some(scBoolVal(as[0] >= as[1])), ">=", 2);
			} else {
				first_arg_type = scTypeGen(args[0].type);
				if (first_arg_type == "int")    ruEvalIntFn(args,    \as -> Some(scBoolVal(as[0] >= as[1])), ">=", 2) else 
				if (first_arg_type == "double") ruEvalDoubleFn(args, \as -> Some(scBoolVal(as[0] >= as[1])), ">=", 2) else 
				ScValue(["operator >= is undefined on arguments: " + ruScValues2s(args)], scErrorType);
				/*switch (args[0]) {
					ScInt(__):    ruEvalIntFn(args,    \as -> Some(ScBool(as[0] >= as[1])), ">=", 2);
					ScDouble(__): ruEvalDoubleFn(args, \as -> Some(ScBool(as[0] >= as[1])), ">=", 2);
					default: ScError(["operator >= is undefined on arguments: " + ruScValues2s(args)]);
				}*/
			}
		}),
		ScriptFn("<", \args, state, out -> {
			if (ruHasStringArg(args)) {
				ruEvalToStringFn(args, \as -> Some(scBoolVal(as[0] < as[1])), "<", 2);
			} else {
				first_arg_type = scTypeGen(args[0].type);
				if (first_arg_type == "int")    ruEvalIntFn(args,    \as -> Some(scBoolVal(as[0] < as[1])), "<", 2) else 
				if (first_arg_type == "double") ruEvalDoubleFn(args, \as -> Some(scBoolVal(as[0] < as[1])), "<", 2) else 
				ScValue(["operator < is undefined on arguments: " + ruScValues2s(args)], scErrorType);
				/*switch (args[0]) {
					ScInt(__):    ruEvalIntFn(args,    \as -> Some(ScBool(as[0] < as[1])), "<", 2);
					ScDouble(__): ruEvalDoubleFn(args, \as -> Some(ScBool(as[0] < as[1])), "<", 2);
					default: ScError(["operator < is undefined on arguments: " + ruScValues2s(args)]);
				}*/
			}
		}),
		ScriptFn(">", \args, state, out -> {
			if (ruHasStringArg(args)) {
				ruEvalToStringFn(args, \as -> Some(scBoolVal(as[0] > as[1])), ">", 2);
			} else {
				first_arg_type = scTypeGen(args[0].type);
				if (first_arg_type == "int")    ruEvalIntFn(args,    \as -> Some(scBoolVal(as[0] > as[1])), ">", 2) else 
				if (first_arg_type == "double") ruEvalDoubleFn(args, \as -> Some(scBoolVal(as[0] > as[1])), ">", 2) else 
				ScValue(["operator > is undefined on arguments: " + ruScValues2s(args)], scErrorType);
				/*switch (args[0]) {
					ScInt(__):    ruEvalIntFn(args,    \as -> Some(ScBool(as[0] > as[1])), ">", 2);
					ScDouble(__): ruEvalDoubleFn(args, \as -> Some(ScBool(as[0] > as[1])), ">", 2);
					default: ScError(["operator > is undefined on arguments: " + ruScValues2s(args)]);
				}*/
			}
		}),
	]
}
