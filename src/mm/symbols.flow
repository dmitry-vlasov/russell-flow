			println("table1: " + symb + ":\n" + ruParse2s(s1));
			s2 = lookupTreeDef(table2, s2id(symb), -1);
			println("table2: " + symb + " -> " + id2s(s2) + "\n----------------\n");
		} else {
			println(symb + " - no such symbol");
		}
	}
	debug("X");*/
	mmSymbTable_ref := Some(table1);
}

