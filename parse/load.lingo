load = '\uFEFF'? ws block*;

block = import | other;
import = "import" ws path ";;" ws;
path = (!";;" char_)+;
other = (!"import" char_)*;

ws = (space | comment)*;
space = ' ' | '\t' | '\n' | '\r';
comment = scomment | mcomment;
scomment = "//" (!'\n' char_)* '\n' ws;
mcomment = "/*" (!"*/" char_)* "*/" ws;
char_ = '\u0000'-'\uFFFF';
