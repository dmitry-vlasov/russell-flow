load = '\uFEFF'? lws lblock*;

lblock = limport | lother;
limport = "import" lws lpath ";;" lws;
lpath = (!";;" lchar)+;
lother = (!"import" lchar)*;

lws = (lspace | lcomment)*;
lspace = ' ' | '\t' | '\n' | '\r';
lcomment = lscomment | lmcomment;
lscomment = "//" (!'\n' lchar)* '\n' lws;
lmcomment = "/*" (!"*/" lchar)* "*/" lws;
lchar = '\u0000'-'\uFFFF';
